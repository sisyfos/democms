@model IEnumerable<CMS.Models.Picture>

@{
    ViewBag.Title = "Index";
}

  <div class="content">
    <div class="innercontent">
<h2>Index</h2>

<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PicTitle)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PicAltText)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.PicTitle)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PicAltText)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.PicID }) |
            @Html.ActionLink("Details", "Details", new { id=item.PicID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.PicID })
        </td>
    </tr>
}

</table>
</div>
</div>